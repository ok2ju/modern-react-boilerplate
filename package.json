{
  "name": "modern-boilerplate",
  "version": "0.1.0",
  "description": "Modern React+Redux starter project",
  "main": "index.js",
  "scripts": {
    "clean:build": "rimraf dist",
    "start": "cross-env NODE_ENV=development webpack-dev-server --config ./config/webpack.config.dev.js",
    "prebuild": "yarn run clean:build",
    "build": "cross-env NODE_ENV=productuin webpack --config ./config/webpack.config.prod.js",
    "lint": "eslint ./src -f table || true",
    "typecheck": "flow",
    "test": "jest"
  },
  "keywords": [
    "react",
    "redux",
    "es2015",
    "babel6",
    "webpack2",
    "flow",
    "babili",
    "preset-env",
    "boilerplace",
    "starter"
  ],
  "author": "Alexey Vakulich <soulSpringg@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "react": "^15.4.2",
    "react-dom": "^15.4.2",
    "react-redux": "^5.0.2",
    "react-router": "^3.0.2",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-core": "^6.22.1",
    "babel-eslint": "^7.1.1",
    "babel-jest": "^18.0.0",
    "babel-loader": "^6.2.10",
    "babel-preset-babili": "^0.0.10",
    "babel-preset-env": "^1.1.8",
    "babel-preset-react": "^6.22.0",
    "cross-env": "^3.1.4",
    "enzyme": "^2.7.1",
    "eslint": "^3.14.1",
    "eslint-config-airbnb": "^14.0.0",
    "eslint-loader": "^1.6.3",
    "eslint-plugin-flowtype": "^2.30.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^3.0.2",
    "eslint-plugin-react": "^6.9.0",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.38.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^18.1.0",
    "react-addons-test-utils": "^15.4.2",
    "react-hot-loader": "3.0.0-beta.6",
    "react-test-renderer": "^15.4.2",
    "rimraf": "^2.5.4",
    "webpack": "2.2.0",
    "webpack-dev-server": "2.2.0",
    "webpack-merge": "^4.1.0"
  }
}
