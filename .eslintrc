{
  "extends": [
    "airbnb"
  ],
  "parser": "babel-eslint",
  "env": {
    "jest": true,
    "browser": true
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "react/forbid-prop-types": [0],
    "import/no-unresolved": 0,
    "jsx-a11y/no-marquee": 0,
    "import/no-extraneous-dependencies": 0,
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
