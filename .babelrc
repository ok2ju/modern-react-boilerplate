{
  "env": {
    "development": {
      "presets": [
        ["env", {
          "modules": false,
          "loose": true,
          "targets": {
            "browsers": ["last 2 versions", "safari 7"]
          }
        }],
        "react"
      ],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    },
    "production": {
      "presets": [
        "babili",
        ["env", {
          "loose": true,
          "targets": {
            "browsers": ["last 2 versions", "safari 7"]
          }
        }],
        "react"
      ],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    },
    "test": {
      "presets": ["env", "react"],
      "plugins": [
        "transform-class-properties",
        "transform-object-rest-spread"
      ]
    }
  }
}
